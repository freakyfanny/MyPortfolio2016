.blurImg     
    position relative
    width 100%
    height 420px
    z-index -1
    top 0
    left 0
    overflow hidden
    & > div
        position fixed
        width 100%
        height 420px
        background-repeat no-repeat
        background-size cover
        background-position center center
        
